<script>
    var gigyaAccountEnabled = true;

    // add Gigya dynamically.
    if(gigyaAccountEnabled){
        ConsentUtility.consent_given(function(){
            $('<script><\/script>')
                    .attr('src', 'https://cdns.gigya.com/JS/gigya.js?apiKey=3_pNK9L9zU_Sx2BKzTUJuAmy1im2zN0pOkwM-Ui3AgrneVzSpyQqioWy_iZ1cbQzS5&lang=nl')
                    .appendTo('body');
        });
    }

    // Sanoma search, also hidden without consent.
    ConsentUtility.consent_given(function() {
        var o = document.createElement('script');
        o.src = 'http://zoeken.startpagina.nl/nu/sanomasearch.min.js';
        if(o.addEventListener){
            o.addEventListener('load', function(){
                if(jQuery('#sanoma_search').length > 0){
                    SANOMASEARCH.init(['autocomplete', 'serp']);
                } else {
                    SANOMASEARCH.init(['autocomplete']);
                }
            });
        } else {
            o.onload = o.onreadystatechange = function() {
                if (!this.readyState || this.readyState === "loaded" || this.readyState === "complete") {
                    if(jQuery('#sanoma_search').length > 0){
                        SANOMASEARCH.init(['autocomplete','serp']);
                    } else {
                        SANOMASEARCH.init(['autocomplete']);
                    }
                }
            };
        }
        document.body.appendChild(o);
    });
</script>

